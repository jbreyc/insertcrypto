  "devdoc": {
    "methods": {
      "activateGame()": {
        "details": "Method that activates a deactivated game so the contract becomes again fully operational."
      },
      "deactivateGame()": {
        "details": "Method that deactivates a game in case an issue is detected.  Functionallity is limited however all data is preserved intact."
      },
      "isActive()": {
        "details": "Method that returns whether the game is active."
      },
      "matchPlayed(address,uint256)": {
        "details": "Method that stores the score attained by the player in the match. Based on the score, the method also calculates the position in the ranking. If the score makes it to the top 10, the method calculates and returns the number of shares attained."
      },
      "playMatch(address)": {
        "details": "Method that flags a purchased match as being played. Once a match is played, it cannot be played again."
      },
      "playerBalance(address)": {
        "details": "Method that returns the balance for a given address."
      },
      "purchaseMatch(address)": {
        "details": "Method to purchase a match in the game. The match is associated to the address passed as a parameter."
      },
      "releaseReward(address)": {
        "details": "Method to releases the balance for a player using a pull payment pattern."
      }
    }
  },
